{
  "name": "neo-skills",
  "description": "A universal capability extension for Gemini CLI",
  "version": "0.22.0",
  "mcpServers": {
    "neo-skills": {
      "command": "node",
      "args": [
        "${extensionPath}/dist/server.js"
      ]
    }
  },
  "contextFileName": "GEMINI.md",
  "hooks": {
    "tool:before_execute": "${extensionPath}/dist/hooks/secret-guard.js"
  }
}